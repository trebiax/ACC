@model List<List<string>>

@{
    ViewData["Title"] = "Report";
}

@if (Model.Count > 0)
{
    <table class="table table-bordered">
        <thead>
            <tr>
                @foreach (var h in Model[0])
                {
                    <th>@h</th>
                }
            </tr>
        </thead>
        <tbody>
            @foreach (var element in Model.Skip(1).ToList())
            {
                <tr>
                    @foreach (var h in element)
                    {
                        <td>@h</td>
                    }
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No report to show</p>
}

